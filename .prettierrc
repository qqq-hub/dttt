{
    "svelteSortOrder": "options-styles-markup-scripts",
    "svelteStrictMode": true,
    "bracketSameLine": true,
    "svelteAllowShorthand": true,
    "svelteIndentScriptAndStyle": true,
    "bracketSpacing": true,
    "htmlWhitespaceSensitivity": "css",
    "insertPragma": false,
    "printWidth": 80,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "requirePragma": false,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
}
